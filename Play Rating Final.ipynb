   ]
  },
  {
   "cell_type": "code",
   "execution_count": 171,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "267052"
      ]
     },
     "execution_count": 171,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "b = []                       #Convertinv all the values to kilobyte or 0.   \n",
    "for i in listA:\n",
    "    if(i[-1] == \"k\"):\n",
    "        integer=i[:-1]\n",
    "        b.append(integer)\n",
    "    elif (i[-1] == \"M\"):\n",
    "        integer=i[:-1]\n",
    "        b.append((integer)*1024)\n",
    "    else:\n",
    "        b.append(i)\n",
    "    \n",
    "len(b)"
   ]
  },
