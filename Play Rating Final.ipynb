   "source": [
    "for_loop_variable = []           #converting all the values to their integer values.\n",
    "haha = 0\n",
    "lala = 0\n",
    "for i in b:\n",
    "    try:\n",
    "        i = i.replace(',','')\n",
    "        for_loop_variable.append(int((float(i) * 100)-(float(i) * 100)%1))\n",
    "        haha += 1\n",
    "    except:\n",
    "        for_loop_variable.append(0)\n",
    "        lala += 1\n",
    "\n",
    "        \n",
    "print(haha,\" lol  \", lala)"
   ]
